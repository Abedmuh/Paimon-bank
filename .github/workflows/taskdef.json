{
  "containerDefinitions": [
    {
      "name": "budimanbank",
      "cpu": 0,
      "portMappings": [
        {
          "name": "budiman-app",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "S3_ID",
          "value": "${{ secrets.S3_ID }}"
        },
        {
          "name": "DB_USERNAME",
          "value": "${{ secrets.DB_USERNAME }}"
        },
        {
          "name": "S3_REGION",
          "value": "${{ secrets.S3_REGION }}1"
        },
        {
          "name": "DB_PORT",
          "value": "${{ secrets.DB_PORT }}"
        },
        {
          "name": "S3_SECRET_KEY",
          "value": "${{ secrets.S3_SECRET_KEY }}"
        },
        {
          "name": "DB_NAME",
          "value": "${{ secrets.DB_NAME }}"
        },
        {
          "name": "S3_BASE_URL",
          "value": "${{ secrets.S3_BASE_URL }}"
        },
        {
          "name": "JWT_TIME_EXP",
          "value": "${{ secrets.JWT_TIME_EXP }}"
        },
        {
          "name": "DB_HOST",
          "value": "${{ secrets.DB_HOST }}"
        },
        {
          "name": "JWT_SECRET_KEY",
          "value": "${{ secrets.JWT_SECRET_KEY }}"
        },
        {
          "name": "DB_PASSWORD",
          "value": "${{ secrets.DB_PASSWORD }}"
        },
        {
          "name": "DB_PARAMS",
          "value": "${{ secrets.DB_PARAMS }}"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/budiman-task",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "budiman-task",
  "taskRoleArn": "arn:aws:iam::560918124458:role/projectsprint_ecs_task_role",
  "executionRoleArn": "arn:aws:iam::560918124458:role/projectsprint_ecs_task_execution_role",
  "networkMode": "awsvpc",
  "volumes": [],
  "status": "ACTIVE",
  "requiresAttributes": [
    {
      "name": "com.amazonaws.ecs.capability.logging-driver.awslogs"
    },
    {
      "name": "ecs.capability.execution-role-awslogs"
    },
    {
      "name": "com.amazonaws.ecs.capability.ecr-auth"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.19"
    },
    {
      "name": "com.amazonaws.ecs.capability.task-iam-role"
    },
    {
      "name": "ecs.capability.execution-role-ecr-pull"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
    },
    {
      "name": "ecs.capability.task-eni"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.29"
    }
  ],
  "placementConstraints": [],
  "compatibilities": ["EC2", "FARGATE"],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "4096",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
